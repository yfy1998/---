<template name="moviestar">
  <view class="star_container">
    <block wx:for="{{stars}}" wx:key="1">
      <image wx:if="{{item}}" src="/images/icon/star.png"></image>
      <image wx:else src="/images/icon/none-star.png"></image>
    </block>
    <text>{{rating.average}}</text>
  </view>
</template>